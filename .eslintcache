avorite = ()=>{\r\n    console.log('cancelFavorite')\r\n    const favoriteAnimal = JSON.parse(localStorage.getItem('animalFavList')) || [];\r\n    const checkDeletFavorNum =  favoriteAnimal?.findIndex(item => item?.AnimalId === AnimalId);\r\n    // debugger;\r\n    if(checkDeletFavorNum !== -1){\r\n      const filterFavoriteAnimal = [...favoriteAnimal.slice(0,checkDeletFavorNum),...favoriteAnimal.slice(checkDeletFavorNum+1)]\r\n      localStorage.setItem('animalFavList',JSON.stringify(filterFavoriteAnimal))\r\n      handleFavor();\r\n    }\r\n  }\r\n  \r\n  return(\r\n    <Card>\r\n      {IsFav ? (<Button onClick={cancelFavorite} className=\"isFavorite\"><Image src={isFav}/></Button>) : (<Button onClick={addFavorite} className=\"isFavorite\"><Image src={isNoFav}/></Button>)}\r\n      \r\n      \r\n       <Link to={`/animalDetail/${AcceptNum}/${AnimalId}`}>\r\n      <div className=\"imgWrapper\">\r\n      <Card.Img variant=\"top\" src={amialPic+pic ||defaultImg} />\r\n      </div>\r\n      \r\n      <Card.Body>\r\n        <Card.Title>\r\n          <h5>{Name || BreedName}</h5>\r\n          <span>{Sex === 1? (<FontAwesomeIcon icon={faMars} size=\"sm\"/>) : (<FontAwesomeIcon icon={faVenus} size=\"sm\"/>) }</span>\r\n        </Card.Title>\r\n        <Card.Text>\r\n          {Message}\r\n        </Card.Text>\r\n        {/* <Button variant=\"primary\">Go somewhere</Button> */}\r\n      </Card.Body>\r\n      </Link>\r\n    </Card>\r\n  )\r\n}\r\n\r\nexport default AnimalCard","D:\\work\\nissa-shulter-project\\src\\pages\\Login.jsx",[],"D:\\work\\nissa-shulter-project\\src\\components\\Header.jsx",["124","125","126","127","128","129","130","131","132","133","134","135","136","137","138"],"import React, { useState, useEffect } from \"react\";\r\nimport {Navbar,Nav,NavItem,Form,FormControl,Button,Dropdown,DropdownButton,Image } from 'react-bootstrap';\r\nimport \"assets/scss/header.scss\";\r\nimport heartIcon  from 'assets/img/icon/love.svg'\r\nimport LogoImg from 'assets/img/